mixin song(song)
    div.song
        span.editBtn
            a(href="") Edit
        img(src='/'+song.imgpath)
        div.song_title
            h4
                a(href=`/music/${song.id}`)=song.title
            span=song.artist 
        span.youtubeIcon
            i.fab.fa-youtube
        span.songView 
            i.fas.fa-headphones  #{song.views}
