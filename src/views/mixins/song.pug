mixin song(song)
    div.song
        img(src='/'+song.imgpath)
        div.song_title
            h4
                a(href=`/music/${song.id}`)=song.title
            div
                span=song.artist 
                span 
                    i.fas.fa-headphones #{song.views}
        button 👍
        span 찜한 수: #{song.like}

        hr